package com.example.newapp;

import android.widget.ImageView;

public class colorObj extends GameSpace{

	ImageView[] orange, white, blue, red, yellow, margenta, green, darkblue;
	
	public colorObj()
	{
		white = new ImageView[2];
		blue = new ImageView[3];
		red = new ImageView[2];
		yellow = new ImageView[2];
		margenta = new ImageView[2];
		green = new ImageView[3];
		darkblue = new ImageView[2];
		orange = new ImageView[2];
		white[0] = (ImageView) findViewById(R.drawable.o_cup_en);
		white[1] = (ImageView) findViewById(R.drawable.o_plate_en);
		blue[0] = (ImageView) findViewById(R.drawable.o_toothbrush_en);
		blue[1] = (ImageView) findViewById(R.drawable.o_toothpaste_en);
		blue[2] = (ImageView) findViewById(R.drawable.o_scissors_en);
		red[0] = (ImageView) findViewById(R.drawable.o_lamp_en);
		red[1] = (ImageView) findViewById(R.drawable.o_doll_en);
		yellow[0] = (ImageView) findViewById(R.drawable.o_fork_en);
		yellow[1] = (ImageView) findViewById(R.drawable.o_gloves_en);
		margenta[0] = (ImageView) findViewById(R.drawable.o_pants_en);
		margenta[1] = (ImageView) findViewById(R.drawable.o_tvset_en);
		green[0] = (ImageView) findViewById(R.drawable.o_album_en);
		green[0] = (ImageView) findViewById(R.drawable.o_train_en);
		green[0] = (ImageView) findViewById(R.drawable.o_socks_en);
		darkblue[0] = (ImageView) findViewById(R.drawable.o_spoon_en);
		darkblue[0] = (ImageView) findViewById(R.drawable.o_phone_en);
		orange[0] = (ImageView) findViewById(R.drawable.o_umbrella_en);
		orange[0] = (ImageView) findViewById(R.drawable.o_bed_en);
	}
}
